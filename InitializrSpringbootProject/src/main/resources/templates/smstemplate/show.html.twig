{% extends 'base.html.twig' %}

{% block body %}
    <h1>Smstemplate</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sMSTemplate.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sMSTemplate.name }}</td>
            </tr>
            <tr>
                <th>Temptype</th>
                <td>{{ sMSTemplate.tempType }}</td>
            </tr>
            <tr>
                <th>Template</th>
                <td>{{ sMSTemplate.template }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ sMSTemplate.userId }}</td>
            </tr>
            <tr>
                <th>Createdon</th>
                <td>{% if sMSTemplate.createdOn %}{{ sMSTemplate.createdOn|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Modifiedon</th>
                <td>{% if sMSTemplate.modifiedOn %}{{ sMSTemplate.modifiedOn|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{% if sMSTemplate.active %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('smstemplate_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('smstemplate_edit', { 'id': sMSTemplate.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
